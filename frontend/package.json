{
  "name": "frontend-2",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "dependencies": {
    "@apollo/client": "3.12.2",
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@babel/register": "7.25.9",
    "@emotion/react": "11.13.5",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "5.16.7",
    "@mui/material": "5.16.7",
    "@types/react-dom": "18.3.1",
    "babel-loader": "9.2.1",
    "dotenv-webpack": "8.1.0",
    "graphql": "16.9.0",
    "graphql-tag": "2.12.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "7.0.2",
    "ts-loader": "9.5.1",
    "typescript": "5.7.2",
    "webpack": "5.97.1",
    "webpack-cli": "5.1.4",
    "webpack-merge": "6.0.1"
  },
  "devDependencies": {
    "@playwright/test": "1.49.0",
    "@types/node": "22.10.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "concurrently": "9.1.0",
    "eslint": "8.57.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "express": "4.21.2",
    "msw": "2.6.8",
    "playwright-msw": "3.0.1",
    "prettier": "3.4.2",
    "typescript-strict-plugin": "2.4.4"
  },
  "scripts": {
    "run-dev-ssr": "webpack --config webpack/config.dev.watch.tsx",
    "build-test": "webpack --config webpack/playwright.tsx",
    "build-prod": "webpack --config webpack/config.production.tsx",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "cicd-lint": "eslint 'src/**/*.tsx'",
    "cicd-typecheck": "tsc-strict --project tsconfig.json --noEmit",
    "cicd-prettier": "yarn prettier --check '**/*.tsx' '**/*.html'",
    "fix": "eslint --fix 'src/**/*.tsx' && prettier . --write",
    "playwright-cicd": "cd tests && playwright test .",
    "playwright-server": "node ./tests/server.js"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  }
}
