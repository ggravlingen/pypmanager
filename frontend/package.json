{
  "name": "frontend-2",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@apollo/client": "3.13.9",
    "@babel/core": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@babel/register": "7.28.3",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.1",
    "@mui/material": "7.3.1",
    "@mui/x-date-pickers": "8.10.2",
    "babel-loader": "10.0.0",
    "chart.js": "4.5.0",
    "dayjs": "1.11.13",
    "dotenv-webpack": "8.1.1",
    "graphql": "16.11.0",
    "graphql-tag": "2.12.6",
    "html-webpack-plugin": "5.6.4",
    "react": "19.1.1",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.1.1",
    "react-router-dom": "7.8.2",
    "ts-loader": "9.5.4",
    "typescript": "5.9.2",
    "webpack": "5.101.3",
    "webpack-cli": "6.0.1",
    "webpack-merge": "6.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.2",
    "@playwright/test": "1.55.0",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "concurrently": "9.2.1",
    "eslint": "9.34.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "54.1.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.2.0",
    "express": "5.1.0",
    "express-rate-limit": "8.0.1",
    "jiti": "2.5.1",
    "msw": "2.10.5",
    "playwright-msw": "3.0.1",
    "prettier": "3.6.2",
    "typescript-strict-plugin": "2.4.4"
  },
  "scripts": {
    "run-dev-ssr": "webpack --config webpack/config.dev.watch.tsx",
    "build-test": "webpack --config webpack/playwright.tsx",
    "build-prod": "find ./static -name '*.js' -exec rm -f {} + && webpack --config webpack/config.production.tsx",
    "build-hass-addon": "find ./static -name '*.js' -exec rm -f {} + && webpack --config webpack/config.production.hass-addon.tsx",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "cicd-lint": "eslint",
    "cicd-typecheck": "tsc-strict --project tsconfig.json --noEmit",
    "fix": "eslint  --fix",
    "playwright-cicd": "playwright test ./tests/",
    "playwright-server": "node ./tests/server.js",
    "playwright-update-snapshots": "playwright test . --update-snapshots"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "packageManager": "yarn@4.9.1"
}
